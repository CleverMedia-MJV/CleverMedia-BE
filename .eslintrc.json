{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier/prettier", "plugin:node/recommended"],
  // "plugin": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-use-before-define": "warn",
    "camelcase": "off",
    "operator-linebreak": "off",
    "func-names": "off",
    "no-unused-vars": "warn",
    "prefer-promise-reject-errors": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off"
  }
}
